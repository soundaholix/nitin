print ("Nitin")
print ("Nitin", "Tarun")
print (5.05)
print (5,5.05)


#one print statement all patterns

print ("$ \n$$ \n$$$ \n$$$$ \n$$$$$")


#one print statement per pattern

print ("$")
print ("$$")
print ("$$$")
print ("$$$$")
print ("$$$$$")



print ("Nitin 37")

print ("Nitin \n37")

print (16 * "*")




